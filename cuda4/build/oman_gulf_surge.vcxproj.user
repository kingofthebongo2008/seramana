<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='release|x64'">
    <LocalDebuggerWorkingDirectory>$(SolutionDir)..\bin\windows\$(Configuration)\</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerEnvironment>PATH=$(VCRedistPaths)%PATH%</LocalDebuggerEnvironment>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='debug|x64'">
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerWorkingDirectory>$(SolutionDir)..\bin\windows\$(Configuration)\</LocalDebuggerWorkingDirectory>
    <LocalDebuggerEnvironment>PATH=$(VCRedistPaths)%PATH%</LocalDebuggerEnvironment>
    <LocalDebuggerDebuggerType>Auto</LocalDebuggerDebuggerType>
  </PropertyGroup>
</Project>